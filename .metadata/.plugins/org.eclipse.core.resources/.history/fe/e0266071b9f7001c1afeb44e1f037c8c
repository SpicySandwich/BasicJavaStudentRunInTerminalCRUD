	public static Map<Integer, StudentEntity> datas(StudentEntity studentEntity){
		
		sMap.put(1, new StudentEntity(studentEntity.setId(1),studentEntity.setName("Conred"),studentEntity.setAge(27)));
		sMap.put(2, new StudentEntity(2,"Batman",28));
		sMap.put(3, new StudentEntity(3,"Superman",14));
		sMap.put(4, new StudentEntity(4,"Joker",16));
		sMap.put(5, new StudentEntity(5,"Alas-as",39));
		
		return sMap;
		
	}
	
	public void getInfoById(Integer id) {
		
		System.out.println(datas(sMap.get(id)));
	}
	
