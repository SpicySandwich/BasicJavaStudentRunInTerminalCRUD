package Student;

import java.util.HashMap;
import java.util.Map;

public class LoginUtil {
	
	HashMap<Class<? extends LoginUtil>, LoginUtil> msMap =  (HashMap<Class<? extends LoginUtil>, LoginUtil>) datas();
	
   public static void main (String[] args) {
		
		ListInfo();
		
	}
	

	public static Map<Class<? extends LoginUtil>, LoginUtil> datas(){
		
		LoginUtil pUtil = new LoginUtil();
		
		msMap.put(pUtil.getClass(),pUtil);
		msMap.put(pUtil.getClass(),pUtil);
		
		return msMap;
		
	}
	
	public static void ListInfo() {
		for(Map.Entry datamap: msMap.entrySet()) {
			System.out.println(datamap.getKey()+" : "+ datamap.getValue());
		}
	}
	
	
	

}
