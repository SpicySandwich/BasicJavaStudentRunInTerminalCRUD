package Student;

import java.util.HashMap;
import java.util.Map;

public class LoginUtil {
	
	static Map<Class<? extends LoginUtil>, LoginUtil> msMap =  (HashMap<Class<? extends LoginUtil>, LoginUtil>) datas();
	
	public static Map<Class<? extends LoginUtil>, LoginUtil> datas(){
		
		LoginUtil pUtil = new LoginUtil();
		
		msMap.put(pUtil.getClass(),pUtil);
		
		return msMap;
		
	}
	
	public LoginUtil getinfo(Class<? extends LoginUtil> clazz) {
		 return msMap.get(clazz);
		
	}

}
