package Student;

public class LoginValidation {
	

	
	
	public void getusername(LoginEntity loginEntity,String username,String password) {
		
	
		 
		 if (!loginEntity.getUsername().equals(username) && !loginEntity.getPassword().equals(password)) 
			 throw new NotMatch("\nPassword not match");
		
		 

		
	}
	
	
	

}
