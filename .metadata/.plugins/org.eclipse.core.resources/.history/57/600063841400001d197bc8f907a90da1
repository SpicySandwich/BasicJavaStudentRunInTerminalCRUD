package Student;

import java.util.HashMap;
import java.util.Map;

public class LoginUtil {
	
	Map<Class<? extends LoginUtil>, LoginUtil> msMap = new HashMap<Class<? extends LoginUtil>, LoginUtil>();
	
	public static Map<Class<? extends LoginUtil>, LoginUtil> datas(){
		
		LoginUtil pUtil = new LoginUtil();
		
		msMap.put(pUtil.getClass(),pUtil);
		
		return msMap;
		
	}

}
